<div class="main shop-show bg-image2">
    <div class="container">
        <div class="shop-return">
            <a href="/shops">
                <p>ショップ一覧へ</p>
            </a>
        </div>
        <div class="shop">
                <div class="shop-image">
                    <%= image_tag(@shopDetails.image_name) %>
                </div>
                <h2 class="shop-name">
                    <%= @shopDetails.name %>
                </h2>
                <h3 class="shop-overview">
                    <%= @shopDetails.overview %>
                <h3 class="shop-url">
                    <%= link_to(@shopDetails.url, @shopDetails.url) %>

        </div>
    </div>
</div>
